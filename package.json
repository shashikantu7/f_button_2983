{
  "name": "froala_editor",
  "description": "Froala WYSIWYG Editor.",
  "license": "https://froala.com/wysiwyg-editor/terms/",
  "version": "3.2.0",
  "startYear": "2014",
  "keywords": [
    "froala",
    "css",
    "html",
    "js"
  ],
  "homepage": "https://www.froala.com/wysiwyg-editor",
  "author": "Froala Labs",
  "scripts": {
    "build-udist": "rm -Rf udist && cp -R dist udist",
    "clean": "rm -Rf  dist/js dist/html dist/css dist/img dist/sass dist/bower.json dist/composer.json dist/editor.jpg dist/index.html dist/License.txt dist/package.json dist/README.md",
    "dist": "npm-run-all clean data-copy data-uglify js-compile data-clean grunt-uglify grunt-copy grunt-themes grunt-sass grunt-banner build-udist grunt-clean-sources grunt-zip grunt-remove",
    "grunt-copy": "grunt copy-files",
    "grunt-clean-sources": "grunt clean-sources",
    "grunt-themes": "grunt createTheme",
    "grunt-sass": "grunt compile-sass",
    "grunt-uglify": "grunt uglify-js",
    "grunt-remove": "grunt remove-sass-pkg-file",
    "grunt-zip": "grunt zip",
    "grunt-banner": "grunt banner",
    "js-compile": "rollup --environment BUNDLE:false --config build/rollup.config.js --sourcemap",
    "js-lint": "eslint src/js/editor.js src/js/index.js src/js/modules src/js/$.js",
    "dev": "webpack-dev-server --config webpack.config.js",
    "es6": "webpack-dev-server --config webpack.es6.config.js",
    "commonjs": "webpack-dev-server --config webpack.commonjs.config.js",
    "dev-test": "webpack-dev-server --config webpack.test.config.js",
    "puppeteer-server": "webpack-dev-server --config webpack.console.config.js",
    "trigger-puppeteer": "wait-on http://localhost:8002 && node puppeteer.js http://localhost:8002/test/index.html",
    "test": "concurrently --kill-others --success first \"npm run puppeteer-server\" \"npm run trigger-puppeteer\" ",
    "data-copy": "cp src/js/modules/core/data.js src/js/modules/core/data.copy.js",
    "data-uglify": "uglifyjs src/js/modules/core/data.js -o src/js/modules/core/data.js -c -m",
    "data-clean": "cp src/js/modules/core/data.copy.js src/js/modules/core/data.js && rm -Rf src/js/modules/core/data.copy.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "capitalize": "^1.0.0",
    "capitalize-first-letter": "^1.0.0",
    "codemirror": "^5.43.0",
    "concurrently": "^4.1.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.10.0",
    "eslint-plugin-compat": "^2.2.0",
    "font-awesome": "^4.7.0",
    "fs": "0.0.1-security",
    "google-translate-api": "^2.3.0",
    "grunt": "^1.0.2",
    "grunt-banner": "^0.6.0",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-compress": "^1.4.3",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-connect": "~0.10.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-jshint": "~1.0.0",
    "grunt-contrib-obfuscator": "^4.0.0",
    "grunt-contrib-qunit": "^2.0.0",
    "grunt-contrib-requirejs": "~0.4.4",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-uglify": "^3.3.0",
    "grunt-jscs": "~3.0.1",
    "grunt-lintspaces": "~0.8.0",
    "grunt-obfuscator": "~0.1.0",
    "grunt-remove-usestrict": "^0.1.1",
    "grunt-sass": "^3.0.2",
    "grunt-saucelabs": "^9.0.0",
    "grunt-shell": "~0.2.2",
    "grunt-slim": "~0.1.0",
    "load-grunt-tasks": "^4.0.0",
    "mathsass": "^0.10.1",
    "npm-run-all": "^4.1.3",
    "puppeteer": "^1.12.2",
    "qunit": "^2.6.1",
    "replace-in-file": "^3.0.0-beta.2",
    "requirejs": "^2.3.5",
    "rollup": "^0.58.2",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-minify-es": "^1.1.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^4.0.0",
    "sinon": "7.1.1",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.0",
    "typescript": "^3.4.5",
    "uglify-es": "^3.3.9",
    "uglify-js": "^3.3.25",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.2",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "babel-loader": "^8.0.5",
    "node-sass": "^4.12.0",
    "safe-regex": "^1.1.0",
    "uglify-js-es6": "^2.8.9",
    "wait-on": "^3.2.0"
  }
}
